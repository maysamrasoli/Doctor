<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:foo="http://schemas.android.com/apk/res-auto"
    xmlns:geekui="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/default_background"
    android:padding="8dp"
    tools:context="ir.tornadoweb.test.MainActivity$PlaceholderFragment">


    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_f_d_home"
                    android:gravity="right"
                    android:orientation="vertical">

                    <ir.medxhub.doctor.util.views.TextViewCustom
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="5dp"
                        android:text="@string/title_home"
                        android:textColor="@color/f_d_h_titles"
                        android:textSize="@dimen/font_size_dash_title"
                        foo:customFont="fonts/iransansmobile.ttf" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="-4dp"
                    android:orientation="vertical"
                    android:background="@drawable/bg_f_d">

                    <RelativeLayout
                        android:id="@+id/free_question"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="1dp"
                        android:layout_marginRight="1dp"
                        android:padding="5dp"
                        android:background="@drawable/btn_dashboard_view"
                        android:clickable="true">

                        <LinearLayout
                            android:id="@+id/linearLayout2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentTop="true"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:layout_margin="5dp"
                                android:src="@drawable/question" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_toLeftOf="@+id/linearLayout2"
                            android:layout_toStartOf="@+id/linearLayout2"
                            android:gravity="right"
                            android:orientation="vertical">

                            <ir.medxhub.doctor.util.views.TextViewCustom
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"

                                android:text="@string/title_free_question"
                                android:textColor="@color/f_d_h_titles"
                                android:textSize="@dimen/font_size_dash_h1"
                                foo:customFont="fonts/iransansmobile_bold.ttf"

                                />

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:orientation="horizontal">

                                <ir.medxhub.doctor.util.views.TextViewCustom
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="2dp"
                                    android:text="پرسیدن سوال به صورت ناشناس"
                                    android:textColor="@color/f_d_h_titles"
                                    android:textSize="@dimen/font_size_dash_h2"
                                    foo:customFont="fonts/iransansmobile_ultralight.ttf"

                                    />

                                <ImageView
                                    android:layout_width="5dp"
                                    android:layout_height="5dp"
                                    android:layout_margin="8dp"
                                    android:background="@drawable/screen2" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:orientation="horizontal">

                                <ir.medxhub.doctor.util.views.TextViewCustom
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="2dp"
                                    android:text="محدودیت تا صدوپنجاه لغت"
                                    android:textColor="@color/f_d_h_titles"
                                    android:textSize="@dimen/font_size_dash_h2"
                                    foo:customFont="fonts/iransansmobile_ultralight.ttf"

                                    />

                                <ImageView
                                    android:layout_width="5dp"
                                    android:layout_height="5dp"
                                    android:layout_margin="8dp"
                                    android:background="@drawable/screen2" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:orientation="horizontal">

                                <ir.medxhub.doctor.util.views.TextViewCustom
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="2dp"
                                    android:text="دریافت اطلاعات آموزشی"
                                    android:textColor="@color/f_d_h_titles"
                                    android:textSize="@dimen/font_size_dash_h2"
                                    foo:customFont="fonts/iransansmobile_ultralight.ttf"

                                    />

                                <ImageView
                                    android:layout_width="5dp"
                                    android:layout_height="5dp"
                                    android:layout_margin="8dp"
                                    android:background="@drawable/screen2" />
                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/linearLayout"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginTop="10dp"
                            android:layout_marginLeft="5dp"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_margin="3dp"
                                android:src="@drawable/arrow_left" />

                            <ir.medxhub.doctor.util.views.TextViewCustom
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/free"
                                android:textColor="@color/f_d_h_titles"
                                android:textSize="@dimen/font_size_dash_h2"
                                foo:customFont="fonts/iransansmobile_bold.ttf"

                                />

                        </LinearLayout>

                    </RelativeLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:background="@drawable/bg_f_d_line"
                        android:orientation="vertical">

                    </LinearLayout>

                    <RelativeLayout
                        android:id="@+id/consultation"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="1dp"
                        android:layout_marginLeft="1dp"
                        android:layout_marginBottom="1dp"
                        android:clickable="true"
                        android:padding="5dp"
                        android:background="@drawable/btn_dashboard_view">

                        <LinearLayout
                            android:id="@+id/linearLayout2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentTop="true"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:layout_margin="5dp"
                                android:src="@drawable/comments16" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_toLeftOf="@+id/linearLayout2"
                            android:layout_toStartOf="@+id/linearLayout2"
                            android:gravity="right"
                            android:orientation="vertical">

                            <ir.medxhub.doctor.util.views.TextViewCustom
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"

                                android:text="@string/title_question"
                                android:textColor="@color/f_d_h_titles"
                                android:textSize="@dimen/font_size_dash_h1"
                                foo:customFont="fonts/iransansmobile_bold.ttf"

                                />

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:orientation="horizontal">

                                <ir.medxhub.doctor.util.views.TextViewCustom
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="2dp"
                                    android:text="پرسیدن سوال به صورت ناشناس"
                                    android:textColor="@color/f_d_h_titles"
                                    android:textSize="@dimen/font_size_dash_h2"
                                    foo:customFont="fonts/iransansmobile_ultralight.ttf"

                                    />

                                <ImageView
                                    android:layout_width="5dp"
                                    android:layout_height="5dp"
                                    android:layout_margin="8dp"
                                    android:background="@drawable/screen2" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:orientation="horizontal">

                                <ir.medxhub.doctor.util.views.TextViewCustom
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="2dp"
                                    android:text="محدودیت تا دو هزار لغت"
                                    android:textColor="@color/f_d_h_titles"
                                    android:textSize="@dimen/font_size_dash_h2"
                                    foo:customFont="fonts/iransansmobile_ultralight.ttf"

                                    />

                                <ImageView
                                    android:layout_width="5dp"
                                    android:layout_height="5dp"
                                    android:layout_margin="8dp"
                                    android:background="@drawable/screen2" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:orientation="horizontal">

                                <ir.medxhub.doctor.util.views.TextViewCustom
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="2dp"
                                    android:text="پیوست فایل و مدارک پزشکی"
                                    android:textColor="@color/f_d_h_titles"
                                    android:textSize="@dimen/font_size_dash_h2"
                                    foo:customFont="fonts/iransansmobile_ultralight.ttf"

                                    />

                                <ImageView
                                    android:layout_width="5dp"
                                    android:layout_height="5dp"
                                    android:layout_margin="8dp"
                                    android:background="@drawable/screen2" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:orientation="horizontal">

                                <ir.medxhub.doctor.util.views.TextViewCustom
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="2dp"
                                    android:text="دریافت مشاوره از متخصص مورد نظر"
                                    android:textColor="@color/f_d_h_titles"
                                    android:textSize="@dimen/font_size_dash_h2"
                                    foo:customFont="fonts/iransansmobile_ultralight.ttf"

                                    />

                                <ImageView
                                    android:layout_width="5dp"
                                    android:layout_height="5dp"
                                    android:layout_margin="8dp"
                                    android:background="@drawable/screen2" />
                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/linearLayout"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginTop="10dp"
                            android:layout_marginLeft="5dp"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_margin="3dp"
                                android:src="@drawable/arrow_left" />

                            <ir.medxhub.doctor.util.views.TextViewCustom
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/buy"
                                android:textColor="@color/f_d_h_titles"
                                android:textSize="@dimen/font_size_dash_h2"
                                foo:customFont="fonts/iransansmobile_bold.ttf"

                                />

                        </LinearLayout>

                    </RelativeLayout>
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_f_d_home"
                    android:gravity="right"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical">

                    <ir.medxhub.doctor.util.views.TextViewCustom
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="5dp"
                        android:text="وقت گرفتن"
                        android:textColor="@color/f_d_h_titles"
                        android:textSize="@dimen/font_size_dash_title"
                        foo:customFont="fonts/iransansmobile.ttf" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="-4dp"
                    android:orientation="vertical"
                    android:background="@drawable/bg_f_d">
                    <RelativeLayout
                        android:id="@+id/appointment"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginBottom="1dp"
                    android:padding="5dp"
                    android:background="@drawable/btn_dashboard_view"
                    android:clickable="true">

                    <LinearLayout
                        android:id="@+id/linearLayout2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_margin="5dp"
                            android:src="@drawable/calendar146" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_toLeftOf="@+id/linearLayout2"
                        android:layout_toStartOf="@+id/linearLayout2"
                        android:gravity="right"
                        android:orientation="vertical">

                        <ir.medxhub.doctor.util.views.TextViewCustom
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:text="گرفتن وقت از مطب"
                            android:textColor="@color/f_d_h_titles"
                            android:textSize="@dimen/font_size_dash_h1"
                            foo:customFont="fonts/iransansmobile_bold.ttf"

                            />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="right"
                            android:orientation="horizontal">

                            <ir.medxhub.doctor.util.views.TextViewCustom
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="2dp"
                                android:text="گرفتن وقت از مطب پزشک"
                                android:textColor="@color/f_d_h_titles"
                                android:textSize="@dimen/font_size_dash_h2"
                                foo:customFont="fonts/iransansmobile_ultralight.ttf"

                                />

                            <ImageView
                                android:layout_width="5dp"
                                android:layout_height="5dp"
                                android:layout_margin="8dp"
                                android:background="@drawable/screen2" />
                        </LinearLayout>

                    </LinearLayout>

                    <ImageView
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:layout_margin="5dp"
                        android:layout_alignBottom="@+id/linearLayout2"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:src="@drawable/arrow_left" />

                </RelativeLayout>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_f_d_home"
                    android:gravity="right"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical">

                    <ir.medxhub.doctor.util.views.TextViewCustom
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="5dp"
                        android:text="امتیاز"
                        android:textColor="@color/f_d_h_titles"
                        android:textSize="@dimen/font_size_dash_title"
                        foo:customFont="fonts/iransansmobile.ttf" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="-4dp"
                    android:orientation="vertical"
                    android:background="@drawable/bg_f_d">
                    <RelativeLayout
                        android:id="@+id/points"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="1dp"
                        android:layout_marginRight="1dp"
                        android:layout_marginBottom="1dp"
                        android:padding="5dp"
                        android:background="@drawable/btn_dashboard_view"
                        android:clickable="true">

                        <ir.medxhub.doctor.util.views.TextViewCustom
                            android:id="@+id/view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_above="@+id/progressBar1"
                            android:layout_marginBottom="3dp"
                            android:layout_marginRight="5dp"
                            android:layout_toLeftOf="@+id/view"
                            android:layout_toStartOf="@+id/view"
                            android:text="80"
                            android:textColor="@color/f_d_h_titles_link"
                            android:textSize="@dimen/font_size_dash_h1"
                            foo:customFont="fonts/yekan.ttf" />

                        <ir.medxhub.doctor.util.views.TextViewCustom
                            android:id="@+id/view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginRight="10dp"
                            android:layout_marginTop="3dp"
                            android:text="مجموع کل امتیازات"
                            android:textColor="@color/f_d_h_titles"
                            android:textSize="@dimen/font_size_dash_h1"
                            foo:customFont="fonts/iransansmobile.ttf" />

                        <ProgressBar
                            android:id="@+id/progressBar1"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"
                            android:layout_below="@+id/view"
                            android:layout_margin="10dp"
                            android:progress="20"
                            android:progressDrawable="@drawable/custom_progressbar" />

                        <ir.medxhub.doctor.util.views.TextViewCustom
                            android:id="@+id/view4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignEnd="@+id/view"
                            android:layout_alignRight="@+id/view"
                            android:layout_below="@+id/progressBar1"
                            android:layout_marginTop="1dp"
                            android:text="سطح"
                            android:textColor="@color/f_d_h_titles"
                            android:textSize="@dimen/font_size_dash_h1"
                            foo:customFont="fonts/iransansmobile.ttf" />

                        <ir.medxhub.doctor.util.views.TextViewCustom
                            android:id="@+id/view5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignTop="@+id/view4"
                            android:layout_marginRight="5dp"
                            android:layout_marginTop="-3dp"
                            android:layout_toLeftOf="@+id/view4"
                            android:layout_toStartOf="@+id/view4"
                            android:text="1"
                            android:textColor="@color/f_d_h_titles_link"
                            android:textSize="@dimen/font_size_dash_h1"
                            foo:customFont="fonts/yekan.ttf" />

                        <ir.medxhub.doctor.util.views.TextViewCustom
                            android:id="@+id/view6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignTop="@+id/view7"
                            android:layout_marginLeft="2dp"
                            android:layout_marginTop="-4dp"
                            android:layout_toEndOf="@+id/view7"
                            android:layout_toRightOf="@+id/view7"
                            android:text="40"
                            android:textColor="@color/f_d_h_titles"
                            android:textSize="@dimen/font_size_dash_h1"
                            foo:customFont="fonts/yekan.ttf" />

                        <ir.medxhub.doctor.util.views.TextViewCustom
                            android:id="@+id/view7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBaseline="@+id/view4"
                            android:layout_alignBottom="@+id/view4"
                            android:layout_alignLeft="@+id/progressBar1"
                            android:layout_alignStart="@+id/progressBar1"
                            android:text="امتیاز تا سطح بعدی"
                            android:textColor="@color/f_d_h_titles"
                            android:textSize="@dimen/font_size_dash_h1"
                            foo:customFont="fonts/iransansmobile.ttf" />


                    </RelativeLayout>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_f_d_home"
                    android:gravity="right"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical">

                    <ir.medxhub.doctor.util.views.TextViewCustom
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="5dp"
                        android:text="یادآوری دارو"
                        android:textColor="@color/f_d_h_titles"
                        android:textSize="@dimen/font_size_dash_title"
                        foo:customFont="fonts/iransansmobile.ttf" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="-4dp"
                    android:orientation="vertical"
                    android:background="@drawable/bg_f_d">

                    <RelativeLayout
                        android:id="@+id/remember"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="1dp"
                        android:layout_marginRight="1dp"
                        android:layout_marginBottom="1dp"
                        android:padding="5dp"
                        android:background="@drawable/btn_dashboard_view"
                        android:clickable="true">

                        <LinearLayout
                            android:id="@+id/linearLayout2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentTop="true"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:layout_margin="5dp"
                                android:src="@drawable/medicines2" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_toLeftOf="@+id/linearLayout2"
